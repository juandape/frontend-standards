{
  "name": "frontend-standards-checker",
  "version": "1.0.0",
  "description": "A comprehensive frontend standards validation tool",
  "type": "module",
  "main": "launcher.cjs",
  "types": "dist/src/index.d.ts",
  "bin": {
    "frontend-standards": "./launcher.cjs",
    "check-frontend-standards": "./launcher.cjs",
    "frontend-standards-checker": "./launcher.cjs"
  },
  "files": [
    "launcher.cjs",
    "bin/",
    "src/",
    "scripts/",
    "tsconfig.json",
    "checkFrontendStandards.config.example.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build-cross-platform.js",
    "build:ts": "tsc",
    "build:executable": "node scripts/build-executable.js",
    "build:cross-platform": "node scripts/build-cross-platform.js",
    "build:production": "NODE_ENV=production node scripts/build-cross-platform.js",
    "dev": "node launcher.cjs",
    "start": "node launcher.cjs",
    "test": "echo \"No tests yet\" && exit 0",
    "test:executable": "node launcher.cjs --help",
    "postinstall": "node scripts/post-install.js",
    "check": "node launcher.cjs",
    "frontend-standards": "node launcher.cjs"
  },
  "keywords": [
    "frontend",
    "standards",
    "validation",
    "typescript",
    "javascript",
    "react"
  ],
  "author": "Frontend Standards Team",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.11.3",
    "acorn-walk": "^8.3.2",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "typescript": "^5.8.3"
  }
}
